{% extends 'base.html.twig' %}

{% block title %}
    {{ page.title }}
{% endblock %}

{% block body %}
<h1>{{ page.title }}</h1>
    {{ page.body }}
<div class="fedback-container">
    <div class="feedback-form">
    {% for message in app.flashes('info') %}
        <p class="alert alert-success">{{ message }}</p>
    {% endfor %}
    {{ form_start(form) }}

    {{ form_widget(form) }}
    <button type="submit" class="btn btn-primary">Отправить </button>

    {{ form_end(form) }}
    </div>
</div>
{% endblock %}

